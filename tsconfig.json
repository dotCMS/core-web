{
    "compileOnSave": false,
    "compilerOptions": {
        "baseUrl": "./",
        "downlevelIteration": true,
        "outDir": "./dist/out-tsc",
        "sourceMap": true,
        "declaration": false,
        "module": "es2020",
        "moduleResolution": "node",
        "emitDecoratorMetadata": true,
        "experimentalDecorators": true,
        "noUnusedLocals": true,
        "noUnusedParameters": true,
        "target": "es2015",
        "typeRoots": ["node_modules/@types"],
        "lib": ["es2017", "dom", "dom.iterable"],
        "paths": {
            "@services/*": ["src/app/api/services/*"],
            "@components/*": ["src/app/view/components/*"],
            "@directives/*": ["src/app/view/directives/*"],
            "@pipes/*": ["src/app/view/pipes/*"],
            "@shared/*": ["src/app/shared/*"],
            "@tests/*": ["src/app/test/*"],
            "@models/*": ["src/app/shared/models/*"],
            "@portlets/*": ["src/app/portlets/*"],
            "dot-layout-grid": ["projects/dot-layout-grid/src/public_api.ts"],
            "dot-layout-grid/*": ["projects/dot-layout-grid/src/public_api.ts"],
            "dot-rules": ["projects/dot-rules/src/public_api.ts"],
            "dot-rules/*": ["projects/dot-rules/src/public_api.ts"],
            "dotcms-js": ["projects/dotcms-js/src/public_api.ts"],
            "dotcms-js/*": ["projects/dotcms-js/src/public_api.ts"],
            "dotcms": ["projects/dotcms/src/public_api.ts"],
            "dotcms/*": ["projects/dotcms/src/public_api.ts"],
            "dotcms-models": ["projects/dotcms-models/src/public_api.ts"],
            "dotcms-models/*": ["projects/dotcms-models/src/public_api.ts"]
        }
    }
}
