{
  "name": "dotcms-ui",
  "version": "5.3.8",
  "license": "MIT",
  "scripts": {
    "ng": "ng",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "test": "ng test",
    "createpackagejson": "node -e \"var data = require('./dist/package.json');var fs = require('fs');data.version = data.version + \"-\" + new Date().getTime();var {scripts, dependencies, devDependencies, ...res} = data;fs.writeFileSync('./dist/package.json', JSON.stringify(res, null, 4));\"",
    "publish:dev": "rm -rf dist;npm run build:libs;npm run build:prod;cp package.json ./dist/package.json;npm run createpackagejson;cd dist;npm publish",
    "publish:doc:docms": "typedoc projects/dotcms/src/lib/ --options projects/dotcms/typedoc.json; gh-pages --dist ./dist-docs/dotcms --dest ./docs/dotcms",
    "publish:doc:docms-models": "typedoc projects/dotcms-models/src/ --options projects/dotcms-models/typedoc.json; gh-pages --dist ./dist-docs/dotcms-models --dest ./docs/dotcms-models",
    "build:dev": "ng build --configuration=development",
    "build:prod": "ng build --configuration=production --prod dotcms-ui",
    "build:libs": "ng build dotcms-js; ng build dot-rules; ng build dot-layout-grid",
    "docker:test": "npm run docker:build;IMAGE_BASE_NAME=core-web docker-compose -f docker-compose-running-test.yml up",
    "docker:build": "docker build -t core-web ."
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/dotCMS/core-web.git"
  },
  "private": false,
  "dependencies": {
    "@angular/animations": "~10.1.4",
    "@angular/cdk": "~7.2.0",
    "@angular/common": "~10.1.4",
    "@angular/compiler": "~10.1.4",
    "@angular/core": "~10.1.4",
    "@angular/forms": "~10.1.4",
    "@angular/platform-browser": "~10.1.4",
    "@angular/platform-browser-dynamic": "~10.1.4",
    "@angular/router": "~10.1.4",
    "@ctrl/tinycolor": "^2.0.0",
    "angular2-hotkeys": "^2.2.0",
    "angular2-tinymce": "^2.1.2",
    "core-js": "^2.5.4",
    "dotcms-webcomponents": "0.0.13",
    "font-awesome": "^4.7.0",
    "lodash": "^4.17.11",
    "md5": "^2.2.1",
    "moment": "^2.19.4",
    "ng2-dragula": "^2.1.0",
    "node-fetch": "^2.6.0",
    "npm": "^6.9.0",
    "primeicons": "^1.0.0",
    "primeng": "^9.1.3",
    "rxjs": "~6.6.3",
    "test": "^0.6.0",
    "tslib": "^2.0.0",
    "zone.js": "~0.10.2"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.1001.4",
    "@angular-devkit/build-ng-packagr": "~0.1001.4",
    "@angular/cli": "~10.1.4",
    "@angular/compiler-cli": "~10.1.4",
    "@angular/language-service": "~10.1.4",
    "@babel/core": "^7.7.2",
    "@types/googlemaps": "3.39.3",
    "@types/jasmine": "~2.8.8",
    "@types/jasminewd2": "~2.0.3",
    "babel-loader": "^8.0.6",
    "codelyzer": "^5.1.2",
    "gh-pages": "^2.0.1",
    "jasmine-core": "~3.5.0",
    "jasmine-spec-reporter": "~5.0.0",
    "karma": "~5.0.0",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage-istanbul-reporter": "~3.0.2",
    "karma-html-reporter": "^0.2.7",
    "karma-jasmine": "~4.0.0",
    "karma-jasmine-html-reporter": "^1.5.0",
    "mock-socket": "^8.0.5",
    "protractor": "~7.0.0",
    "ts-node": "~7.0.0",
    "tslint": "~6.1.0",
    "tslint-angular": "^1.1.1",
    "typescript": "~4.0.3"
  }
}
