{
  "name": "dotcms-ui",
  "version": "4.2.1",
  "license": "MIT",
  "scripts": {
    "ng": "ng",
    "start": "ng serve --proxy-config proxy.conf.json --port 3000",
    "build": "ng build --prod --aot=false --base-href=\"./\"",
    "watch": "npm run build -w --no-delete-output-path --sourcemap",
    "test": "ng test -sr",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "devversionbump": "node -e \"var data = require('./dist/package.json');var fs = require('fs');data.version = data.version + \"-\" + new Date().getTime();fs.writeFileSync('./dist/package.json', JSON.stringify(data, null, 4));\"",
    "publishdev": "rm -rf dist;npm run build;cp package.json ./dist/package.json;npm run devversionbump;cd dist;npm publish",
    "dotcms": "npm run build -- -w --sourcemap=false"
  },
  "private": false,
  "dependencies": {
    "@angular/animations": "^4.2.4",
    "@angular/common": "^4.2.4",
    "@angular/compiler": "^4.2.4",
    "@angular/core": "^4.2.4",
    "@angular/forms": "^4.2.4",
    "@angular/http": "^4.2.4",
    "@angular/platform-browser": "^4.2.4",
    "@angular/platform-browser-dynamic": "^4.2.4",
    "@angular/router": "^4.2.4",
    "angular2-grid": "^2.1.0",
    "angular2-hotkeys": "^2.0.3",
    "angular2-logger": "^0.5.1",
    "angular2-tinymce": "^2.1.2",
    "core-js": "^2.4.1",
    "dotcms-rules-engine": "^0.0.5",
    "lodash": "4.16.6",
    "md5": "^2.2.1",
    "moment": "2.14.1",
    "ng2-ace-editor": "^0.2.5",
    "ng2-dragula": "1.3.1",
    "primeng": "^4.0.1",
    "rxjs": "^5.4.2",
    "zone.js": "^0.8.14"
  },
  "devDependencies": {
    "@angular/cli": "1.3.0",
    "@angular/compiler-cli": "^4.2.4",
    "@angular/language-service": "^4.2.4",
    "@types/googlemaps": "^3.26.7",
    "@types/jasmine": "~2.5.53",
    "@types/jasminewd2": "~2.0.2",
    "@types/node": "~6.0.60",
    "codelyzer": "~3.1.1",
    "dotcms-js": "^4.2.0-b93",
    "font-awesome": "^4.7.0",
    "jasmine-core": "~2.6.2",
    "jasmine-spec-reporter": "~4.1.0",
    "karma": "~1.7.0",
    "karma-chrome-launcher": "~2.1.1",
    "karma-cli": "~1.0.1",
    "karma-coverage-istanbul-reporter": "^1.2.1",
    "karma-jasmine": "~1.1.0",
    "karma-jasmine-html-reporter": "^0.2.2",
    "protractor": "~5.1.2",
    "ts-node": "~3.2.0",
    "tslint": "~5.3.2",
    "typescript": "~2.3.3"
  }
}
