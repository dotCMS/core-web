{
    "name": "dotcms-ui",
    "version": "5.0.1",
    "license": "MIT",
    "scripts": {
        "ng": "ng",
        "start": "ng serve --proxy-config proxy.conf.json --port 3000",
        "build:dev": "ng build -v -w --dev --aot=false --base-href=\"./\"",
        "build": "ng build --prod --aot=false --base-href=\"./\"",
        "watch": "npm run build -w --no-delete-output-path --sourcemap",
        "test": "ng test --sm=false",
        "lint": "ng lint",
        "e2e": "ng e2e",
        "devversionbump": "node -e \"var data = require('./dist/package.json');var fs = require('fs');data.version = data.version + \"-\" + new Date().getTime();fs.writeFileSync('./dist/package.json', JSON.stringify(data, null, 4));\"",
        "publishdev": "rm -rf dist;npm run build;cp package.json ./dist/package.json;npm run devversionbump;cd dist;npm publish",
        "dotcms": "npm run build:dev --sourcemap=true --"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/dotCMS/core-web.git"
    },
    "private": false,
    "dependencies": {
        "@angular/animations": "^5.2.11",
        "@angular/common": "^5.2.11",
        "@angular/compiler": "^5.2.11",
        "@angular/core": "^5.2.11",
        "@angular/forms": "^5.2.11",
        "@angular/http": "^5.2.11",
        "@angular/platform-browser": "^5.2.11",
        "@angular/platform-browser-dynamic": "^5.2.11",
        "@angular/platform-server": "^5.2.11",
        "@angular/router": "^5.2.11",
        "@ctrl/tinycolor": "^2.0.0",
        "angular2-grid": "^2.1.0",
        "angular2-hotkeys": "^2.0.3",
        "angular2-tinymce": "^2.1.2",
        "core-js": "^2.5.3",
        "dotcms-rules-engine": "^0.0.7",
        "lodash": "^4.17.4",
        "md5": "^2.2.1",
        "moment": "^2.19.4",
        "ng2-ace-editor": "^0.3.8",
        "ng2-dragula": "^1.5.0",
        "primeng": "^4.3.0",
        "rxjs": "^5.5.11",
        "zone.js": "^0.8.18"
    },
    "devDependencies": {
        "@angular/cli": "^1.6.0",
        "@angular/compiler-cli": "^5.2.11",
        "@angular/language-service": "^4.4.6",
        "@types/googlemaps": "^3.30.1",
        "@types/jasmine": "^2.8.2",
        "@types/jasminewd2": "^2.0.3",
        "@types/node": "^6.0.93",
        "codelyzer": "~3.1.1",
        "dotcms-js": "file:../dotcms-js/build-dotcms-js/dotcms-js-4.2.0-b97.tgz",
        "font-awesome": "^4.7.0",
        "jasmine-core": "~2.6.2",
        "jasmine-spec-reporter": "~4.1.0",
        "karma": "^1.7.1",
        "karma-chrome-launcher": "~2.1.1",
        "karma-cli": "~1.0.1",
        "karma-coverage-istanbul-reporter": "^1.2.1",
        "karma-jasmine": "^1.1.1",
        "karma-jasmine-html-reporter": "^0.2.2",
        "node-sass": "^4.9.0",
        "protractor": "~5.1.2",
        "ts-node": "~3.2.0",
        "tslint": "~5.9.1",
        "tslint-angular": "^1.1.1",
        "typescript": "2.4.2",
        "webpack": "^3.10.0"
    }
}
